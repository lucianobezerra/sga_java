	<queryString>
		<![CDATA[SELECT
     users.`id`,
     users.`login`,
     users.`nome`,
     users.`email`,
     windows.`description`
FROM
     `users` users INNER JOIN `users_windows` users_windows ON users.`id` = users_windows.`user_id`
